<definitions xmlns:xs="http://www.w3.org/2001/XMLSchema"
             xmlns:tns="http://example.com/webservice"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             targetNamespace="http://example.com/webservice">

    <types>
        <xs:schema targetNamespace="http://example.com/webservice">
            <!-- Veri türü tanımlamaları buraya eklenir -->
        </xs:schema>
    </types>

    <message name="GetUserDataInput">
        <part name="userID" type="xs:int"/>
    </message>

    <message name="GetUserDataOutput">
        <part name="userData" type="xs:string"/>
    </message>

    <portType name="MyWebServicePortType">
        <operation name="GetUserData">
            <input message="tns:GetUserDataInput"/>
            <output message="tns:GetUserDataOutput"/>
        </operation>
        <!-- Diğer operasyonlar buraya eklenir -->
    </portType>

    <binding name="MyWebServiceBinding" type="tns:MyWebServicePortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="GetUserData">
            <soap:operation soapAction="GetUserData"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>
        <!-- Diğer operasyonlar buraya eklenir -->
    </binding>

    <service name="MyWebService">
        <port name="MyWebServicePort" binding="tns:MyWebServiceBinding">
            <soap:address location="http://example.com/MyWebService"/>
        </port>
    </service>
</definitions>